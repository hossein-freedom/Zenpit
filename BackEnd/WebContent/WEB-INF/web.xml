<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <servlet>
	  <servlet-name>storeDevice</servlet-name>
	  <servlet-class>
	     util.storeDevice
	  </servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>storeDevice</servlet-name>
      <url-pattern>/storeDeviceData</url-pattern>
  </servlet-mapping>
  
  <servlet>
	  <servlet-name>retrievDevice</servlet-name>
	  <servlet-class>
	     util.retrievDevice
	  </servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>retrievDevice</servlet-name>
      <url-pattern>/retrievDeviceData</url-pattern>
  </servlet-mapping>
</web-app>